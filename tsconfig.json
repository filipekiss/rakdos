{
    "compilerOptions": {
        "target": "es5",
        "moduleResolution": "node",
        "allowJs": true,
        "noEmit": true,
        "strict": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "*": ["*", "src/*"],
            "static": ["static/*"]
        }
    },
    "include": ["src"]
}
